{
  "name": "cypress-do-zero-a-nuvem",
  "version": "1.0.0",
  "description": "Curso 'Cypress, do Zero à Nuvem' da Escola Talking About Testing.",
  "main": "src/index.html",
  "amazon": "https://www.amazon.com.br/",
  "scripts": {
    "start": "",
    "cy:open": "cypress open",
    "test": "cypress run",
    "cy:mobile": "cypress run --config viewportWidth=410,viewportHeight=860",
    "cy:mobile:open": "cypress open --config viewportWidth=410,viewportHeight=860",
    "test:mobile": "cypress run --config viewportWidth=410,viewportHeight=860",
    "test:cloud": "cypress run --record",
    "cy:run": "cypress run --config video=false --env allure=true",
    "allure:generate": "allure generate allure-results",
    "allure:open": "allure open allure-report",
    "allure:clear": "rimraf allure-results allure-report",
    "test:allure": "npx npm-run-all allure:clear cy:run allure:generate",
    "test:desafio": "cypress run --spec 'cypress/e2e/desafio-tecnico-code.cy.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/cypress-do-zero-a-nuvem.git"
  },
  "keywords": [
    "Testing",
    "Cypress.io",
    "Talking About Testing",
    "Cypress, do Zero à Nuvem",
    "Cypress Cloud",
    "GitHub Actions",
    "Escola TAT"
  ],
  "author": "Walmyr <wlsf82@gmail.com> (https://walmyr.dev/)",
  "license": "MIT",
  "homepage": "https://github.com/wlsf82/cypress-do-zero-a-nuvem#readme",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "cypress": "^13.12.0",
    "rimraf": "^6.0.1"
  }
}
